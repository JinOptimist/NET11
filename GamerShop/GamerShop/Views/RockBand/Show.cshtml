@using GamerShop.Models.RockHall;
@model List<InfoBandViewModel>

<div>
    <h1><u>Bands of the Rock and Roll Hall of Fame:</u></h1><br />
    @foreach (var rockBand in Model)
    {
        <div>
            <h2>
                @rockBand.FullName
            </h2>
        </div>
        <div>
            <a asp-action="Delete" asp-controller="RockBand" asp-route-id="@rockBand.Id">Удалить</a>
        </div>
        <div>
            Создал: @rockBand.CreatorName
        </div>
    }
</div>